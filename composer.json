{
  "name": "lostfocus/jf2",
  "type": "library",
  "description": "Basic mapping and handling of jf2 files and objects",
  "homepage": "https://github.com/lostfocus/jf2",
  "license": "MIT",
  "authors": [
    {
      "name": "Dominik Schwind",
      "email": "dschwind@lostfocus.de",
      "homepage": "https://lostfocus.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "8.2.*|8.3.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": ">=11.0"
  },
  "autoload": {
    "psr-4": {
      "Lostfocus\\Jf2\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lostfocus\\Jf2\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "lock": false,
    "sort-packages": true
  }
}
